<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swuljcityconductor">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SWULJCityConductor">
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <activity android:name=".FaresActivity"></activity>
        <activity android:name=".StopActivity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".QrCodeResultActivity"
            android:label="@string/title_activity_qr_code_result"
            android:theme="@style/Theme.SWULJCityConductor.NoActionBar" />
        <activity
            android:name=".GenerateSingleActivity"
            android:label="@string/title_activity_generate_single"
            android:theme="@style/Theme.SWULJCityConductor.NoActionBar" />
        <activity
            android:name=".GenerateBulkActivity"
            android:label="@string/title_activity_generate_bulk"
            android:theme="@style/Theme.SWULJCityConductor.NoActionBar" />
        <activity
            android:name=".GenerateQrCodeActivity"
            android:label="@string/title_activity_generate_qr_code"
            android:theme="@style/Theme.SWULJCityConductor.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.SWULJCityConductor.NoActionBar" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>